SELECT * FROM Sales; - - Displayed the sales table


SELECT c.First_Name, c.Last_Name, SUM(s.Total_Price) AS Total_Spent
FROM Customers c
JOIN Sales s ON c.Customer_ID = s.Customer_ID
GROUP BY c.First_Name, c.Last_Name
ORDER BY Total_Spent DESC
LIMIT 1; - - Diplayed the customer who spent the total amount of money


SELECT c.First_Name, c.Last_Name, COUNT(*) AS Total_Items_Bought
FROM Customers c
JOIN Sales s ON c.Customer_ID = s.Customer_ID
GROUP BY c.First_Name, c.Last_Name
ORDER BY Total_Items_Bought DESC
LIMIT 3; - - Diplayed the top 3 customers who made the most purchases 


SELECT Product_Name, Quantity 
FROM Products
JOIN Sales
ON Products.Product_ID = Sales.Product_ID
GROUP BY Product_Name
ORDER BY Quantity DESC
LIMIT 3; - - Displayed the products that were sold the most 
